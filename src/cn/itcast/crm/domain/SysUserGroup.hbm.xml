<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.crm.domain.SysUserGroup" table="sys_user_group">
		<id name="id" type="integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="name" type="string">
			<column name="name" sql-type="varchar(100)" />
		</property>

		<property name="principle" type="string">
			<column name="principle" sql-type="varchar(50)" />
		</property>

		<property name="inclument" type="string">
			<column name="inclument" sql-type="varchar(200)" />
		</property>

		<property name="remark" type="text">
			<column name="remark" />
		</property>

	</class>
</hibernate-mapping>
